    dc.b    $01,$02,$02,$08     ; C
    dc.b    $05,$06,$07,$08     ; T

    dc.b    $09,$0A,$0A,$0B     ; P     ; 2nd line
    dc.b    $09,$0A,$0C,$0B     ; R
    dc.b    $0D,$00,$00,$0D     ; O
    dc.b    $00,$00,$00,$0D     ; J
    dc.b    $09,$0A,$0A,$0E     ; E
    dc.b    $0D,$00,$00,$00     ; C
    dc.b    $00,$0F,$10,$00     ; T

    dc.b    $11,$00,$00,$00     ; P     ; 3rd line
    dc.b    $11,$00,$12,$13     ; R
