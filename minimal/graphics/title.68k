;--------------------;

TitlePatterns:
    dc.b    $01,$02,$02,$03     ; P     ; 1st line
    dc.b    $01,$02,$02,$03     ; R
    dc.b    $01,$02,$02,$03     ; O
    dc.b    $00,$00,$00,$04     ; J
